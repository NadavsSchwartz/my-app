  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",

      "android": {
        // compile the whole dev APK and disable Kotlinâ€™s strict null-checks
        "gradleCommand": ":app:assembleDebug -PdisableStrictNullChecks"
      }
